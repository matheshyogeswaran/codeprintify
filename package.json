{
  "name": "codeprintify",
  "displayName": "CodePrintify",
  "description": "Copy project folder structure or code files with a single click. Save the folder structure and code content as `.txt` files for easy sharing and documentation.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeprintify.helloWorld",
    "onCommand:extension.copyFolderStructure",
    "onCommand:extension.copyCodeFiles"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeprintify.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.copyFolderStructure",
        "title": "Copy Folder Structure"
      },
      {
        "command": "extension.copyCodeFiles",
        "title": "Copy Code Files"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
